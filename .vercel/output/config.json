{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/_actions(?:/(.*?))?/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/rangosConfig/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/variablesConfig/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/seed/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/update-rangos/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/update-variables/?$",
			"dest": "_render"
		},
		{
			"src": "^/cliente/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/cliente(?:/(.*?))?/?$",
			"dest": "_render"
		},
		{
			"src": "^/configuracion/?$",
			"dest": "_render"
		},
		{
			"src": "^/metricasGenerales/?$",
			"dest": "_render"
		},
		{
			"src": "^/vistaClientes/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	]
}